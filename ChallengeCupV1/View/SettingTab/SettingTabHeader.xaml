             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ChallengeCupV1.View.SettingTab"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
